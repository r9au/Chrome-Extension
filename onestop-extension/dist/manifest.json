{
    "name":"One-stop resource manager",
    "short_name":"Onestop mang",
    "version":"0.1.0",
    "description":"Manage your projects and resources",
    "manifest_version":3,
    "action":{
        "default_popup":"popup.html",
        "default_icon":"icon.jpg"
    },
    "permissions":[
        "storage","activeTab","tabs"
    ],
    "host_permmissions":["<all_urls>"],
    "content_scripts":[{
        "matches":["<all_urls>"],
        "js":["content.js"],
        "type":"module"
    }],
    "author":"Rj",
    "homepage_url":"https://localhost:5173"
}